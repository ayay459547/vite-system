{
  "name": "vite-system",
  "version": "1.0.7",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx vite --force --mode development",
    "build:dev": "run-p type-check build-only:dev",
    "build-only:dev": "vite build --mode development",
    "pro": "npx vite --mode production",
    "build:pro": "run-p type-check build-only:pro",
    "build-only:pro": "vite build --mode production",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "run-p vite-build",
    "vite-build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "type-check": "vue-tsc --build",
    "eslint-check": "eslint . --fix-dry-run",
    "eslint-debug": "eslint . --debug",
    "prettier": "prettier --check src/",
    "open-profile": "vite --profile open",
    "transform-debug": "vite --debug transform",
    "transform-excel": "npx tsx transformExcel_production.ts",
    "licenses-json": "npx license-checker --production --json > ./src/licenses.json",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@dagrejs/dagre": "^1.1.5",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@vavt/cm-extension": "^1.10.1",
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.2",
    "@vue-flow/core": "^1.45.0",
    "@vue-flow/minimap": "^1.5.3",
    "@vue-flow/node-resizer": "^1.5.0",
    "@vue-flow/node-toolbar": "^1.1.1",
    "@vueuse/components": "^13.5.0",
    "@vueuse/core": "^13.5.0",
    "axios": "^1.10.0",
    "chokidar": "^4.0.3",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.5.1",
    "element-plus": "^2.10.4",
    "exceljs": "^4.4.0",
    "gh-pages": "^6.3.0",
    "idb": "^8.0.3",
    "ifvisible.js": "^1.0.6",
    "interactjs": "^1.10.27",
    "js-cookie": "^3.0.5",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lodash-es": "^4.17.21",
    "md-editor-v3": "^5.8.2",
    "pinia": "^3.0.3",
    "sweetalert2": "^11.22.2",
    "uuid": "^11.1.0",
    "vue": "^3.5.17",
    "vue-i18n": "^11.1.10",
    "vue-qr": "^4.0.9",
    "vue-router": "^4.5.1",
    "vuedraggable": "^4.1.0",
    "vxe-table": "~4.8.16",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@interactjs/types": "^1.10.27",
    "@tsconfig/node22": "^22.0.2",
    "@types/crypto-js": "^4.2.2",
    "@types/js-cookie": "^3.0.6",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.16.5",
    "@types/uuid": "^10.0.0",
    "@vicons/antd": "^0.13.0",
    "@vicons/carbon": "^0.13.0",
    "@vicons/fa": "^0.13.0",
    "@vicons/fluent": "^0.13.0",
    "@vicons/ionicons4": "^0.13.0",
    "@vicons/ionicons5": "^0.13.0",
    "@vicons/material": "^0.13.0",
    "@vicons/tabler": "^0.13.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^14.5.2",
    "eslint": "^9.31.0",
    "eslint-plugin-cypress": "^4.3.0",
    "eslint-plugin-oxlint": "^0.16.12",
    "eslint-plugin-vue": "^10.3.0",
    "jsdom": "^26.1.0",
    "license-checker": "^25.0.1",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^0.16.12",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.89.2",
    "start-server-and-test": "^2.0.12",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^6.3.5",
    "vite-plugin-lazy-import": "^1.0.7",
    "vite-plugin-vue-devtools": "^7.7.7",
