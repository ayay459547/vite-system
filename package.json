{
  "name": "rt-aps",
  "version": "1.0.7",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx vite --force --mode development",
    "build:dev": "run-p type-check build-only:dev",
    "build-only:dev": "vite build --mode development",
    "pro": "npx vite --mode production",
    "build:pro": "run-p type-check build-only:pro",
    "build-only:pro": "vite build --mode production",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "run-p transform-excel vite-build",
    "vite-build": "vite build --",
    "transform-excel": "node transformExcel.js",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix-dry-run",
    "format": "prettier --check src/",
    "profile": "vite --profile open",
    "transform": "vite --debug transform"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@vavt/cm-extension": "^1.6.0",
    "@vueuse/components": "^12.2.0",
    "@vueuse/core": "^12.2.0",
    "axios": "^1.7.9",
    "chokidar": "^4.0.3",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.5.1",
    "element-plus": "^2.9.1",
    "exceljs": "^4.4.0",
    "idb": "^8.0.1",
    "js-cookie": "^3.0.5",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.4",
    "md-editor-v3": "^5.1.1",
    "pinia": "^2.3.0",
    "sweetalert2": "^11.15.3",
    "uuid": "^11.0.3",
    "vue": "^3.5.13",
    "vue-i18n": "^11.0.1",
    "vue-qr": "^4.0.9",
    "vue-router": "^4.5.0",
    "vuedraggable": "^4.1.0",
    "vxe-table": "^4.7.79",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/js-cookie": "^3.0.6",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "@vicons/antd": "^0.12.0",
    "@vicons/carbon": "^0.12.0",
    "@vicons/fa": "^0.12.0",
    "@vicons/fluent": "^0.12.0",
    "@vicons/ionicons4": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/material": "^0.12.0",
    "@vicons/tabler": "^0.12.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/eslint-plugin": "1.1.20",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^13.17.0",
    "eslint": "^9.14.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-vue": "^9.30.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.3.3",
    "sass": "^1.83.0",
    "start-server-and-test": "^2.0.9",
    "typescript": "~5.6.3",
    "vite": "^6.0.5",
    "vite-plugin-lazy-import": "^1.0.7",
    "vite-plugin-vue-devtools": "^7.6.8",
