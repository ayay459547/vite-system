{
  "name": "demo",
  "version": "1.0.7",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx vite --force --mode development",
    "build:dev": "run-p type-check build-only:dev",
    "build-only:dev": "vite build --mode development",
    "pro": "npx vite --mode production",
    "build:pro": "run-p type-check build-only:pro",
    "build-only:pro": "vite build --mode production",
    "start": "vite --force",
    "staging": "npx vite --mode staging",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "type-check:all": "run-p type-check:*",
    "type-check:app": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "type-check:node": "vue-tsc --noEmit -p tsconfig.node.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prettier": "prettier --write src/",
    "format": "run-p prettier lint",
    "profile": "vite --profile",
    "transform": "vite --debug transform"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@kangc/v-md-editor": "^2.3.18",
    "@types/js-cookie": "^3.0.6",
    "@vueuse/components": "^11.1.0",
    "@vueuse/core": "^10.11.1",
    "axios": "^1.7.7",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.13",
    "echarts": "^5.5.1",
    "element-plus": "^2.8.3",
    "exceljs": "^4.4.0",
    "idb": "^7.1.1",
    "js-cookie": "^3.0.5",
    "pinia": "^2.2.2",
    "sweetalert2": "^11.14.0",
    "uuid": "^9.0.1",
    "vue": "^3.5.6",
    "vue-i18n": "^9.14.0",
    "vue-qr": "^4.0.9",
    "vue-router": "^4.4.5",
    "vuedraggable": "^4.1.0",
    "vxe-table": "^4.7.79",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^18.19.50",
    "@vicons/antd": "^0.12.0",
    "@vicons/carbon": "^0.12.0",
    "@vicons/fa": "^0.12.0",
    "@vicons/fluent": "^0.12.0",
    "@vicons/ionicons4": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/material": "^0.12.0",
    "@vicons/tabler": "^0.12.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^13.14.2",
    "eslint": "^8.57.1",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-vue": "^9.28.0",
    "jsdom": "^21.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "sass": "^1.79.1",
    "start-server-and-test": "^2.0.8",
    "typescript": "~5.0.4",
    "vite": "^4.5.5",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-lazy-import": "^1.0.7",
    "vite-plugin-vue-devtools": "^7.4.5",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.2.0"
  }
}
